{
    "ServiceSpec": {
        "Name": "rfid-query-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "rfid-query-agent:1.1.0",
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:8080/rfid-query-agent/",
            "externalPath": "/rfid-query-agent/"
        }
    }
}
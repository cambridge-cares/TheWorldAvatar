                    "type": "geojson",
                    "cluster": false,
                    "clusterMaxZoom": 14,
                    "clusterRadius": 100,
                    "data": "http://localhost:3838/geoserver/kingslynn/ows?service=WFS&version=1.0.0&request=GetFeature&typeName=kingslynn%3Atsp&outputFormat=application%2Fjson"
                },

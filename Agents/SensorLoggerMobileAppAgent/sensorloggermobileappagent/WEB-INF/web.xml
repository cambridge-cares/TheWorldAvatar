<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" metadata-complete="false" version="3.1">
    <display-name>SensorLoggerMobileAppAgent</display-name>
    <description>A java agent to receives sensor information from SensorLoggerMobileApp.</description>

    <servlet>
        <servlet-name>SensorLoggerMobileAppAgent</servlet-name>
        <servlet-class>uk.ac.cam.cares.jps.agent.sensorloggermobileappagent.sensorloggermobileappagent</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SensorLoggerMobileAppAgent</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>


</web-app>
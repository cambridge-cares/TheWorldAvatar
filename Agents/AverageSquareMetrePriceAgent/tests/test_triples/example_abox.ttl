################################################
## Average Square Metre Price TESTS instances ##
################################################

# The Average Square Metre Price Agent takes the following 3 pure inputs:
#   obe:hasPostalCode
#   obe:PropertyPriceIndex
#   lrppi:TransactionRecord
#
# They need timestamp to be properly retrieved by the derivation agent.
# However, the timestamp is not provided in the example triples. Instead, 
# they will be added by the script when the derivation instances are created
# for new info

:Building_1
    rdf:type dabgeo:Building ; 
    obe:hasAddress :Address_1 ; 
    obe:hasLatestTransactionRecord :Transaction_1 ;
    obe:hasTotalFloorArea :Area_1 .
:Transaction_1 rdf:type lrppi:TransactionRecord ;
               lrppi:transactionDate "2000-06-20"^^xsd:date ;
               lrppi:pricePaid "300000"^^xsd:integer .
:Area_1 rdf:type om:Area ;
        om:hasValue :Measure_1 .
:Measure_1 rdf:type om:Measure ;
           om:hasNumericalValue "280.0"^^xsd:float .
:Address_1 rdf:type icontact:Address ;
           obe:hasPostalCode :PostalCode_1 ;
           obe:hasAdministrativeDistrict :District_1 .

:Building_2
    rdf:type dabgeo:Building ; 
    obe:hasAddress :Address_2 ; 
    obe:hasLatestTransactionRecord :Transaction_2 ;
    obe:hasTotalFloorArea :Area_2 .
:Transaction_2 rdf:type lrppi:TransactionRecord ;
               lrppi:transactionDate "2006-01-13"^^xsd:date ;
               lrppi:pricePaid "210000"^^xsd:integer .
:Area_2 rdf:type om:Area ;
        om:hasValue :Measure_2 .
:Measure_2 rdf:type om:Measure ;
           om:hasNumericalValue "130.0"^^xsd:float .
:Address_2 rdf:type icontact:Address ;
           obe:hasPostalCode :PostalCode_1 ;
           obe:hasAdministrativeDistrict :District_1 .

:Building_3
    rdf:type dabgeo:Building ; 
    obe:hasAddress :Address_3 ; 
    obe:hasLatestTransactionRecord :Transaction_3 ;
    obe:hasTotalFloorArea :Area_3 .
:Transaction_3 rdf:type lrppi:TransactionRecord ;
               lrppi:transactionDate "1993-11-25"^^xsd:date ;
               lrppi:pricePaid "90000"^^xsd:integer .
:Area_3 rdf:type om:Area ;
        om:hasValue :Measure_3 .
:Measure_3 rdf:type om:Measure ;
           om:hasNumericalValue "220.0"^^xsd:float .
:Address_3 rdf:type icontact:Address ;
           obe:hasPostalCode :PostalCode_1 ;
           obe:hasAdministrativeDistrict :District_1 .

:Building_4
    rdf:type dabgeo:Building ; 
    obe:hasAddress :Address_4 ; 
    obe:hasLatestTransactionRecord :Transaction_4 ;
    obe:hasTotalFloorArea :Area_4 .
:Transaction_4 rdf:type lrppi:TransactionRecord ;
               lrppi:transactionDate "1998-08-08"^^xsd:date ;
               lrppi:pricePaid "260000"^^xsd:integer .
:Area_4 rdf:type om:Area ;
        om:hasValue :Measure_4 .
:Measure_4 rdf:type om:Measure ;
           om:hasNumericalValue "240.0"^^xsd:float .
:Address_4 rdf:type icontact:Address ;
           obe:hasPostalCode :PostalCode_2 ;
           obe:hasAdministrativeDistrict :District_1 .

:Building_5
    rdf:type dabgeo:Building ; 
    obe:hasAddress :Address_5 ; 
    obe:hasLatestTransactionRecord :Transaction_5 ;
    obe:hasTotalFloorArea :Area_5 .
:Transaction_5 rdf:type lrppi:TransactionRecord ;
               lrppi:transactionDate "2020-04-05"^^xsd:date ;
               lrppi:pricePaid "500000"^^xsd:integer .
:Area_5 rdf:type om:Area ;
        om:hasValue :Measure_5 .
:Measure_5 rdf:type om:Measure ;
           om:hasNumericalValue "140.0"^^xsd:float .
:Address_5 rdf:type icontact:Address ;
           obe:hasPostalCode :PostalCode_2 ;
           obe:hasAdministrativeDistrict :District_1 .

:PostalCode_1 rdf:type obe:PostalCode ;
              rdfs:label "ABC 123"^^xsd:string .
:PostalCode_2 rdf:type obe:PostalCode ;
              rdfs:label "DEF 456"^^xsd:string .
:District_1 rdf:type obe:AdministrativeDistrict .

:PropertyPriceIndex_1 rdf:type obe:PropertyPriceIndex ; 
                      obe:representativeFor :District_1 .



{
    "ServiceSpec": {
        "Name": "trajectoryqueryagent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/trajectoryqueryagent:3.0.0",
                "Env": [
                    "KEYCLOAK_SERVER=http://host.docker.internal:8080",
                    "KEYCLOAK_REALM=twa-test",
                    "JAVA_TOOL_OPTIONS=-agentlib:jdwp=transport=dt_socket,server=y,suspend=y,address=*:5005"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    },
                    {
                        "ConfigName": "geoserver"
                    },
                    {
                        "ConfigName": "user-agent-rest"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    },
                    {
                        "SecretName": "geoserver_password"
                    }
                ]
            }
        },
        "EndpointSpec": {
            "Ports": [
                {
                    "Name": "web",
                    "Protocol": "tcp",
                    "TargetPort": "5005",
                    "PublishedPort": "5005"
                }
            ]
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:8080/trajectoryqueryagent/",
            "externalPath": "/trajectoryqueryagent/"
        }
    }
}
{
    "ServiceSpec": {
        "Name": "pubchem-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/pubchem_agent:1.0.0",
                "Env": [
                    "NAMESPACE=ontospecies"
                ],

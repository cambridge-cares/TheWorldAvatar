{
    "ServiceSpec": {
        "Name": "pubchem-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/pubchem_agent:1.0.0",
                "Env": [
                    "NAMESPACE=ontospecies4",
                    "QUERY_ENDPOINT=http(s)://<your-blazegraph-host>/blazegraph/namespace/ontospecies4/sparql",
                    "UPDATE_ENDPOINT=http(s)://<your-blazegraph-host>/blazegraph/namespace/ontospecies4/sparql"                    
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:5000",
            "externalPath": "/pubchemagent"
        }
    }
}
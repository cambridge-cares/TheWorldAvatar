{
    "ServiceSpec": {
        "Name": "cea-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/cea-agent:3.0.0",
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    },
                    {
                        "ConfigName": "ontop"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "cea-agent": {
          "url": "http://localhost:8084/cea-agent/",
          "externalPath": "/cea-agent/"
        }
    }
}
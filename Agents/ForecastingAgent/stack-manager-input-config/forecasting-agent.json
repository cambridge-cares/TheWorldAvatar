{
    "ServiceSpec": {
        "Name": "forecasting_agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/forecasting_agent:1.1.1",
                "Env": [
                    "NAMESPACE=excel",
                    "DATABASE=excel"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:5000/",
            "externalPath": "/forecastingAgent/"
        }
    }
}
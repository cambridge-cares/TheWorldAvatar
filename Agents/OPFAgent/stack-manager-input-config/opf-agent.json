{
    "ServiceSpec": {
        "Name": "opf-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "opf-agent:1.1.0",
                "Env": [
                    "LOG4J_FORMAT_MSG_NO_LOOKUPS=true"
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:8080/opf-agent/",
            "externalPath": "/opf-agent/"
        }
    }
}

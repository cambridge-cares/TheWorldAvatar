{
    "ServiceSpec": {
        "Name": "access-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/access-agent:1.7.0",
                "Env": [
                    "LOG4J_FORMAT_MSG_NO_LOOKUPS=true",
                    "STOREROUTER_ENDPOINT=http://<STACK NAME>-blazegraph:8080/blazegraph/namespace/kb/sparql",
                    "RDB_STOREROUTER_ENDPOINT=http://<STACK NAME>-blazegraph:8080/blazegraph/namespace/storerouterrdb/sparql"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "ontop"
                    },
                    {
                        "ConfigName": "postgis"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:8080/access-agent/",
            "externalPath": "/access-agent/"
        }
    }
}

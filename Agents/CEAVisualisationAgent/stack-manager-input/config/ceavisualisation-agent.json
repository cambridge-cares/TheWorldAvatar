{
    "ServiceSpec": {
        "Name": "ceavisualisation-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/ceavisualisation-agent:0.2.0-SNAPSHOT",
                "Env": [
                    "DB_NAME=postgres"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "cea-agent": {
          "url": "http://localhost:8080/ceavisualisation-agent/",
          "externalPath": "/ceavisualisation-agent/"
        }
    }
}
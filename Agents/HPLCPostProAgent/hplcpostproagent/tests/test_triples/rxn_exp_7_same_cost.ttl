################################
## Reaction experiment data 6 ##
################################
# The purpose of this additional data is to make sure the HPLCPostProAgent produces the correct cost calculation

doe_instance:HistoricalData_1 OntoDoE:refersToExperiment exp7:RxnExp_1.

exp7:RxnExp_1 OntoRxn:isAssignedTo dummy_post_proc:VapourtecR4_2. # assigned to dummy_post_proc:VapourtecR4_2

SinglePhase:Phase_1 OntoCAPEMat:representsThermodynamicBehavior exp7:InputChemical_1.
SinglePhase:Phase_2 OntoCAPEMat:representsThermodynamicBehavior exp7:InputChemical_2.
SinglePhase:Phase_3 OntoCAPEMat:representsThermodynamicBehavior exp7:InputChemical_3.

exp7:RxnExp_1
    rdf:type OntoRxn:ReactionExperiment;
    OntoRxn:isOccurenceOf ChemRxn:ChemRxn_1;
    OntoRxn:hasResTime exp7:ResidenceTime_1;
    OntoRxn:hasRxnTemperature exp7:RxnTemperature_1;
    OntoRxn:hasRxnPressure exp7:RxnPressure_1;
    OntoRxn:hasStoichiometryRatio exp7:StoiRatio_1;
    OntoRxn:hasStoichiometryRatio exp7:StoiRatio_2;
    OntoRxn:hasStoichiometryRatio exp7:StoiRatio_3;
    OntoRxn:hasRxnScale exp7:RxnScale_1;
    OntoRxn:hasInputChemical exp7:InputChemical_1;
    OntoRxn:hasInputChemical exp7:InputChemical_2;
    OntoRxn:hasInputChemical exp7:InputChemical_3;
.

exp7:ResidenceTime_1
    rdf:type OntoRxn:ResidenceTime;
    om:hasPhenomenon exp7:RxnExp_1;
    om:hasValue exp7:ResidenceTime_1_Measure_1;
.

exp7:ResidenceTime_1_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:minute-Time;
    om:hasNumericalValue "8.1"^^xsd:double;
.

exp7:RxnTemperature_1
    rdf:type OntoRxn:ReactionTemperature;
    om:hasPhenomenon exp7:RxnExp_1;
    om:hasValue exp7:RxnTemperature_1_Measure_1;
.

exp7:RxnTemperature_1_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:degreeCelsius;
    om:hasNumericalValue "56"^^xsd:double;
.

exp7:RxnPressure_1
    rdf:type OntoRxn:ReactionPressure;
    om:hasPhenomenon exp7:RxnExp_1;
    om:hasValue exp7:RxnPressure_1_Measure_1;
.

exp7:RxnPressure_1_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:bar;
    om:hasNumericalValue "6"^^xsd:double;
.

exp7:InputChemical_1
    rdf:type OntoRxn:InputChemical;
    OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_1;
.

exp7:InputChemical_2
    rdf:type OntoRxn:InputChemical;
    OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_2;
.

exp7:InputChemical_3
    rdf:type OntoRxn:InputChemical;
    OntoCAPEMat:thermodynamicBehavior SinglePhase:Phase_3;
.

exp7:StoiRatio_1
    rdf:type OntoRxn:StoichiometryRatio;
    OntoDoE:positionalID "http://www.theworldavatar.com/kg/ontospecies/Species_54d8b46b-17bc-4bbd-a3cc-3b3a16d6ae4b"^^xsd:string;
    om:hasPhenomenon exp7:RxnExp_1;
    OntoRxn:indicatesMultiplicityOf exp7:InputChemical_1;
    om:hasValue exp7:StoiRatio_1_Measure_1;
.

exp7:StoiRatio_1_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:one;
    om:hasNumericalValue "1.0"^^xsd:double;
.

exp7:StoiRatio_2
    rdf:type OntoRxn:StoichiometryRatio;
    OntoDoE:positionalID "http://www.theworldavatar.com/kg/ontospecies/Species_353d4667-e25d-476a-bd74-5c34723c8ea3"^^xsd:string;
    om:hasPhenomenon exp7:RxnExp_1;
    OntoRxn:indicatesMultiplicityOf exp7:InputChemical_2;
    om:hasValue exp7:StoiRatio_2_Measure_1;
.

exp7:StoiRatio_2_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:one;
    om:hasNumericalValue "5.01"^^xsd:double;
.

exp7:StoiRatio_3
    rdf:type OntoRxn:StoichiometryRatio;
    OntoDoE:positionalID "http://www.theworldavatar.com/kg/ontospecies/Species_eab77458-560d-4ce9-9b5e-96650fc3e202"^^xsd:string;
    om:hasPhenomenon exp7:RxnExp_1;
    OntoRxn:indicatesMultiplicityOf exp7:InputChemical_3;
    om:hasValue exp7:StoiRatio_3_Measure_1;
.

exp7:StoiRatio_3_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:one;
    om:hasNumericalValue "0.17"^^xsd:double;
.

exp7:RxnScale_1
    rdf:type OntoRxn:ReactionScale;
    OntoDoE:positionalID "http://www.theworldavatar.com/kg/ontospecies/Species_54d8b46b-17bc-4bbd-a3cc-3b3a16d6ae4b"^^xsd:string;
    om:hasPhenomenon exp7:RxnExp_1;
    OntoRxn:indicatesUsageOf exp7:InputChemical_1;
    om:hasValue exp7:RxnScale_1_Measure_1;
.

exp7:RxnScale_1_Measure_1
    rdf:type om:Measure;
    om:hasUnit om:millilitre;
    om:hasNumericalValue "5"^^xsd:double;
.

PREFIX fh: <http://www.theworldavatar.com/ontology/OntoFHRS/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?feature ?name ?type ?postcode ?ratingValue ?ratingDate ?latitude ?longitude
WHERE {
  ?feature fh:isPartOfDomain <http://www.theworldavatar.com/ontology/OntoFHRS/FoodHygieneRating> ;
           fh:hasBusinessName ?name ;
           fh:hasBusinessType ?type ;
           fh:hasPostcode ?postcode ;
           fh:hasRatingValue ?ratingValue ;
           fh:hasRatingDate ?ratingDate .
  ?location fh:isPartOfDomain <http://www.theworldavatar.com/ontology/OntoFHRS/FoodHygieneRating> ;
            fh:hasLongitude ?longitude ;
            fh:hasLatitude ?latitude .
}

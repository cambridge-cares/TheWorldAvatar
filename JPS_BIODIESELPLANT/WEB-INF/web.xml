<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" metadata-complete="false" version="3.1">
    <display-name>CrossDomainQueryAgent</display-name>
    <description>This agent executes a cross-domain query to access/filter items based on their geo-spatial and
        * chemical engineering concepts, simultaneously, that are present in different knowledge graphs.</description>

    <servlet>
        <servlet-name>deployCrossDomainQuery</servlet-name>
        <servlet-class>uk.ac.cam.cares.jps.bio.CrossDomainQueryAgent</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>deployCrossDomainQuery</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#hasShapeRepresentation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#hasShapeRepresentation">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasAspectSystem"/>
        <owl:inverseOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsShapeOf"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Solid"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points from a System to the Solid that represents its geometry</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#hasSurfaceGeometry -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#hasSurfaceGeometry">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasAspectSystem"/>
        <owl:inverseOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsSurfaceGeometryOf"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Surface"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points from a System to the Surface that represents its geometry</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_area -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_area">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Solid"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Surface"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#SurfaceArea"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">workaround for QCR which is a feature of modeling currently not available from OWL.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_length -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_length">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Solid"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Surface"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Diameter"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#EdgeLength"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Height"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Radius"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">workaround for QCR which is a feature of modeling currently not available from OWL.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_volume -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#has_volume">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Solid"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Volume"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">workaround for QCR which is a feature of modeling currently not available from OWL.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsShapeOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsShapeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#representsAspectOf"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Solid"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points from a Solid to the System whose geometry the Solid represents</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsSurfaceGeometryOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#representsSurfaceGeometryOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#representsAspectOf"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontocape/supporting_concepts/geometry/geometry.owl#Surface"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">points from a Surface to the System whose geometry the Surface represents</rdfs:comment>
    </owl:ObjectProperty>
    



    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasAssignedBin -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasAssignedBin">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/ContractLifecycleEventOccurrence"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/Bin"/>
        <rdfs:comment>A relationship assigning the bin to a service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has assigned bin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasAssignedDriver -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasAssignedDriver">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/VC/VehicleCore/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/Employee"/>
        <rdfs:comment>A relationship describing the vehicle and its assigned driver.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has assigned driver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasBinType -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasBinType">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/BinDeliveryService"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/WasteCollectionService"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/Bin"/>
        <rdfs:comment>A relationship describing the bin type for a series of waste services.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has bin type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasDisposalCharge -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasDisposalCharge">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/WasteDisposalService"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontoservice/VariableCharge"/>
        <rdfs:comment>A relationship describing the disposal charge of a waste disposal service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has disposal charge</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasStatus -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasStatus">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/Bin"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/BinStatus"/>
        <rdfs:comment>A relationship describing the current bin status.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasWasteType -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasWasteType">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/WasteCollectionService"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/Waste"/>
        <rdfs:comment>A relationship describing the waste category for a series of waste services.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has waste type</rdfs:label>
    </owl:ObjectProperty>
    



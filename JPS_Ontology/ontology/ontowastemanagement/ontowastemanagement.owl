    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasGrossWeight -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasGrossWeight">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/GarbageTruck"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/GrossWeight"/>
        <rdfs:comment>A relationship describing the gross truck weight during the waste disposal procedure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has gross weight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontowastemanagement/hasMeasurementTime -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/hasMeasurementTime">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/GrossWeight"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontowastemanagement/UnladenWeight"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.w3.org/TR/owl-time/#time:Instant"/>
        <rdfs:comment>A relationship which describes the time of truck weight measurement with waste during a job.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has measurement time</rdfs:label>
    </owl:ObjectProperty>
    



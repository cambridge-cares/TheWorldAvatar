        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/NetWeight"/>
        <rdfs:comment>A relationship describing the net weight of the waste collected during the service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontowastemanagement/"/>
        <rdfs:label>has net weight</rdfs:label>
    </owl:ObjectProperty>
    



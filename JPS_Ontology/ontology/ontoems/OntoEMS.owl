        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has downstream station</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontoems/hasForecastedValue -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontoems/hasForecastedValue">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontoems/Forecast"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a quantity and its forecast.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has forecasted value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontoems/includedIn -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontoems/includedIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontoems/DiffuseHorizontalIrradiance"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontoems/DirectNormalIrradiance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontoems/GlobalHorizontalIrradiance"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an individual measure and a sum of measures.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">included in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontoems/measureOf -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontoems/measureOf">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontoems/AirPollutantConcentration"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iot/vocab/m3-lite#AirPollution"/>
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/WeatherOntology.owl#AirPollution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an individual measure and a measure aggregate (e.g. individual air pollutant concentration and overall air quality index).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">measure of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontoems/reports -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontoems/reports">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontoems/ReportingStation"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a reporting entity and the quantity it reports.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">reports</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontotimeseries/hasTimeSeries -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontotimeseries/hasTimeSeries">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontoems/Forecast"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontotimeseries/TimeSeries"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a measure or forecast and the respective time series.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has time series</rdfs:label>
    </owl:ObjectProperty>
    



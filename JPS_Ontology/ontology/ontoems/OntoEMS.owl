        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship between a river level reporting station and the associated stage scale</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stage scale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit -->


    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontoems/Forecast"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Unit"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a forecast and the respective unit of measure.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a measure and the respective unit of measure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/"/>
        <rdfs:isDefinedBy rdf:resource="https://www.theworldavatar.com/kg/ontoems/"/>

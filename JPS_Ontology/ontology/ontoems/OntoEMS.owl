    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontoems/hasDownstreamStation">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontoems/WaterLevelReportingStation"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontoems/WaterLevelReportingStation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship between a water level reporting station and its next equivalent downstream</rdfs:comment>

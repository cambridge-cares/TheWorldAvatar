     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.theworldavatar.com/kg/ontocrystal/">
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">22 April 2024</dc:date>
        <gitCommitHash rdf:datatype="http://www.w3.org/2001/XMLSchema#string">c0599beca8df55873a1ab061dee64e52c510c6a0</gitCommitHash>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OntoCrystal is an ontology developed for representing crystal structure information</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasAbscissaData -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasAbscissaData">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PlotXY"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AbscissaData"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The X axis of the xRD spectrum. In crystallography the axis is 2*theta, and measured in degrees.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Abscissa Data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasAtomSite -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasAtomSite">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomicStructure"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomSite"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects AtomicStructure with individual AtomSites.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Atom Site</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasAtomicStructure -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasAtomicStructure">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CrystalInformation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomicStructure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects CrystalInformation class with its AtomicStructure, which is information about all containing atoms.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Atomic Structure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasAuthor -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasAuthor">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/Citation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/Author"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links the publication (Citation) and the author. This replaces the standard bibo:author_list</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasAuthorIndex -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasAuthorIndex">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/Citation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AuthorIndex"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The link between the publication and the Author.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Author Index</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasCartesianPosition -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasCartesianPosition">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomSite"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PositionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Similarly, relates an AtomSite with a Vector, but in this case, the position is given in fractional coordinates.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Cartesian Position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasCharacteristicPeak -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasCharacteristicPeak">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/XRDSpectrum"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CharacteristicPeak"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Property to link the XRD spectrum and with each of its characteristic peaks</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Characteristic Peak</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasCoordinateTransformation -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasCoordinateTransformation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CrystalInformation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CoordinateTransformation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a CIF to the collection of CoordinateTransformation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Coordinate Transformation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasCrystalInformation -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasCrystalInformation">
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CrystalInformation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relation property to specify the Crystallization Information of a material. It should be used by an external ontology.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Crystal Information</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasElement -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasElement">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomSite"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The specific atom site in the crystal lattice is occupied by a particular type of atom or chemical species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasFractionalPosition -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasFractionalPosition">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomSite"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PositionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an AtomSite to a Vector representing its position in Cartesian coordinates.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Fractional Position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasMatrixComponent -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasMatrixComponent">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/MeasureMatrix"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/MatrixComponent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Associates a Matrix with its MatrixComponents. Matrix has multiple components.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Matrix Component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasMillerIndices -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasMillerIndices">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/CharacteristicPeak"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/MillerIndices"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Direction of reflection plane, that generated the characteristic peak. Optional parameter, may not be avaiable for experimentally measured spectrum.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Miller Indices</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasOrdinateData -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasOrdinateData">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PlotXY"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/OrdinateData"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Y axis of the XRD spectrum. In crystallography the axis is in arbitrary units. Here in percent of the highest peak (range 0 to 100).</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Ordinate Data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasPlotData -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasPlotData">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/XRDSpectrum"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PlotXY"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Property to link the XRD spectrum and its 2D plot.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Plot</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasPosition -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasPosition">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/TileVertex"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/MeasureVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cartesian coordinates of the vertex in the tile face. Collection of all vertices can reproduce the entire tile face in 3D.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellAngles -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellAngles">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCell"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCellAngles"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects a UnitCell to a Vector representing the angles between the axes in its reciprocal space. The reciprocal space is a mathematical construct used in crystallography and solid-state physics. The Vector labels are typically alpha*, beta*, gamma*, the Units of measure is degrees.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Reciprocal Unit Cell Angles</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellLengths -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellLengths">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCell"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCellLengths"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects a UnitCell with a Vector that stores the lengths of its axes in the reciprocal space. The Vector labels are typically a*, b*, c*, the Units of measure reciprocalAngstrom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Reciprocal Unit Cell Lengths</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellVectorSet -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasReciprocalUnitCellVectorSet">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCell"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/UnitCellVectorSet"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a UnitCell to a set of vectors (ReciprocalUnitCellVectorSet) that define the three vectors in reciprocal space.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Reciprocal Unit Cell Vector Set</rdfs:label>
    </owl:ObjectProperty>
    



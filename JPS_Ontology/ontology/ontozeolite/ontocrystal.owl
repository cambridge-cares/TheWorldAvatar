<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.theworldavatar.com/kg/ontocrystal/"
     xml:base="http://www.theworldavatar.com/kg/ontocrystal/"
     xmlns:ontocrystal="http://www.theworldavatar.com/kg/ontocrystal/"

    <owl:Class rdf:about="http://www.theworldavatar.com/kg/ontocrystal/AtomicStructure">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/hasAtomSite"/>
                <owl:allValuesFrom rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/AtomSite"/>
            </owl:Restriction>

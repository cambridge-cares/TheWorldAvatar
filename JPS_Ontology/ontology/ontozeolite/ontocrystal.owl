    <!-- http://www.theworldavatar.com/kg/ontocrystal/hasTransformationMatrixToCartesian -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasTransformationMatrixToCartesian">

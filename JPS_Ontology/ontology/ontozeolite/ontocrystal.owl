    <owl:Class rdf:about="http://www.theworldavatar.com/kg/ontocrystal/AtomSite">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/hasCartesianPosition"/>
                <owl:allValuesFrom rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PositionVector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/hasElement"/>
                <owl:allValuesFrom rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/hasFractionalPosition"/>
                <owl:allValuesFrom rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/PositionVector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Information of an in individual atom within a crystallographic unit cell. It stores the 3D coordinates, atom type, and possibly other properties of the atom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Atom Site</rdfs:label>
    </owl:Class>
    



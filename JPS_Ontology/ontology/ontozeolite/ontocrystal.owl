    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasTileFaceNumber">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/Tile"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/TileFaceNumber"/>

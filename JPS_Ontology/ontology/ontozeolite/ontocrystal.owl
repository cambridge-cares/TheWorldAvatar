    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasTileNumber">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/TiledStructure"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/TileNumber"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Number of tiles in a crystal unit cell.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Tile Number</rdfs:label>
    </owl:ObjectProperty>
    



    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/kg/ontocrystal/hasTileVertex">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/Tile"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/kg/ontocrystal/TileVertex"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Information about all vertices of the given Tile Face. In cartesian coordinates.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/kg/ontocrystal/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has Tile Vertex</rdfs:label>

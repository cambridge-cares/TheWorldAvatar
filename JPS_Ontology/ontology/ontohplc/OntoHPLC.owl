    <!-- https://www.theworldavatar.com/kg/ontohplc/ChromatogramPoint -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontohplc/ChromatogramPoint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontohplc/atRetentionTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontohplc/RetentionTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontohplc/hasPeakArea"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontohplc/PeakArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One data point that indicates a peak appears in the chromatogram, normally it refers to a phase component.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontohplc/</rdfs:isDefinedBy>
    </owl:Class>
    



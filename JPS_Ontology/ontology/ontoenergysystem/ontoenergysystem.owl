     -->


    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#Property -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#Property">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#ScalarValue -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#ScalarValue"/>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#System -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#System">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoeip/powerplants/PowerPlant.owl#PowerPlant -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoeip/powerplants/PowerPlant.owl#PowerPlant">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Asset"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoeip/powerplants/PowerPlant.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#AdministrativeDivision -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#AdministrativeDivision">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A generic name for geographical areas into which a particular, independent sovereign state (country) is divided. Such a unit usually has an administrative authority with the power to take administrative or policy decisions for its area.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Asset -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Asset">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical item of property, which is of value and available to meet debts, commitments, or legacies. Normally, asset is persisted by a natural person, company or any organisations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#CommercialElectricityConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#CommercialElectricityConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Non-DomesticElectricityConsumption"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasValue"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#ScalarValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One of the components of the Non-DomesticElectricityConsumption which is totally different from IndustrialElectricityConsumption. It specifically refers to the electricity consumed the by nonmanufacturing establishments or agencies primarily engaged in the sale of goods or services. In short, CommercialElectricityConsumption includes the consumption of the service sector. It is highly connected to population growth (as services tend to population serving).</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#DomesticElectricityConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#DomesticElectricityConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricityConsumption"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasValue"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#ScalarValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The rest part of TotalElectricityConsumption which refers to the electricity used by domestic purposes, conceptually opposites to Non-DomesticElectricityConsumption.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricPowerSystem -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricPowerSystem">
        <owl:equivalentClass rdf:resource="http://www.theworldavatar.com/ontology/ontopowsys/PowSysRealization.owl#PowerSystem"/>
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergySystem"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A network of electrical components deployed to supply, transfer, and use electric power.  An example of a power system is the electrical grid that provides power to homes, industries, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricityConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricityConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The form of energy consumption that uses electric energy. Electric energy consumption is the actual energy demand made on existing electricity supply which is not equal to the nominal demanding.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The amount of energy or power used. The consumer can be of any types includes a given area. </rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergySystem -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergySystem">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#System"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A system primarily designed to supply energy-services to end-users. All components of energy system are related to the production, conversion, delivery, and use of energy. The energy type can be of variety includes both primary and secondary energy sources.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A demarcated area of the Earth.The boundary of the area is of arbitrary which means it can be official, like the border of country,  or non-offical ones forming from any conventional rules or even the boundaries with no underlying meaning.  </rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#IndustrialElectricityConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#IndustrialElectricityConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Non-DomesticElectricityConsumption"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasValue"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#ScalarValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One of the components of the Non-DomesticElectricityConsumption, which refers to the electric energy is predominantly used for industrial purpose, such as production, as well as for lighting and other business uses in the manufacturing industries.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#NaturalGasSystem -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#NaturalGasSystem">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergySystem"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A transmission network (system) of gas pipelines that supply gas to end-users from natural gas terminals situated on the coast, and to gas distribution companies that supply commercial and domestic users.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Non-DomesticElectricityConsumption -->


    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Non-DomesticElectricityConsumption">
        <rdfs:subClassOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricityConsumption"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#includesConsumption"/>
                <owl:allValuesFrom>
                    <owl:Class>

<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#"
     xml:base="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl"
     xmlns:OntoEnergySystem="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl">
        <owl:imports rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">18 November 2021</dc:date>
        <gitCommitHash rdf:datatype="http://www.w3.org/2001/XMLSchema#string">c0599beca8df55873a1ab061dee64e52c510c6a0</gitCommitHash>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OntoEnergySystem is an ontology developed for representing energy systems.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.bigdata.com/rdf/geospatial/literals/v1#lat-lon -->


    <rdfs:Datatype rdf:about="http://www.bigdata.com/rdf/geospatial/literals/v1#lat-lon"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasValue -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasValue">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#enablesElectricityConsumptionOf -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#enablesElectricityConsumptionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#enablesEnergyConsumptionOf"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricPowerSystem"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#ElectricityConsumption"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation links an EnergySystem with the electic energy consumption which is met by it. </rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#enablesEnergyConsumptionOf -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#enablesEnergyConsumptionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#hasProperty"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergySystem"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation links an EnergySystem with the energy consumption which is met by it. </rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasRelevantPlace -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasRelevantPlace">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#Asset"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an asset and its relevant areas. Normally it refers to the area where the asset located in.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#includesConsumption -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#includesConsumption">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#EnergyConsumption"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a total consumption and a sub-consumption which is the inverse of isSub-ConsumptionOf</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#isObservedIn -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#isObservedIn">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#Property"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a phenomena and its observed object. It is the inverse property of observes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#observes -->


    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#observes">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#GeographicalArea"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation is the inverse of isObservedIn.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasCode -->


    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasCode">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a GeographicalArea and its tag of any types of codes or indexes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasLocalAuthorityCode -->


    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasLocalAuthorityCode">
        <rdfs:subPropertyOf rdf:resource="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasCode"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between AdministrativeDivision and Land Authority code which it is tagged.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasWGS84LatitudeLongitude -->


    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl#hasWGS84LatitudeLongitude">
        <rdfs:range rdf:resource="http://www.bigdata.com/rdf/geospatial/literals/v1#lat-lon"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an object and its Latitude-Longitude coordinates (basde on EPSG 4326, also known as WGS84).</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontoenergysystem/OntoEnergySystem.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //

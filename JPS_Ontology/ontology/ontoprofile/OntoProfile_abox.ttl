@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix op: <https://www.theworldavatar.com/kg/ontoprofile/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

op:WorkResponsibilityEnum a op:WorkResponsibility ;
  owl:oneOf (
    op:ServiceContact
) .

op:ServiceContact a op:WorkResponsibilityEnum ;
  rdfs:label "contact";
  rdfs:comment "In charge of being the main point of contact for a specific service".

op:EmployeeStatusEnum a op:EmployeeStatus ;
owl:oneOf (
  op:OnLeave
  op:OnWork
) .

op:OnLeave a op:EmployeeStatusEnum ;
  rdfs:label "On leave";
  rdfs:comment "This status indicates that the employee is on leave.".

op:OnWork a op:EmployeeStatusEnum ;
  rdfs:label "On work";
  rdfs:comment "This status indicates that the employee is working on another task.".
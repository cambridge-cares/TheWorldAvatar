<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#"
     xml:base="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:OntoSpecies="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#">
    <owl:Ontology rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl">
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">13 February 2020</dc:date>
        <gitCommitHash rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ce46cbbad4054a9c725889bee0278e90abc3b167</gitCommitHash>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OntoSpecies is an ontology developed for representing chemical species.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#gitCommitHash -->

    <owl:AnnotationProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#gitCommitHash"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasElement -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasElement">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines that a species or molecular entity contains a chemical element.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasElementNumber -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasElementNumber">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines a combined representation of a number indicating the amount of an element in a species and the element.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#indicatesNumberOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#indicatesNumberOf">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasEmpiricalFormula -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasEmpiricalFormula">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a species or molecular entity and the chemical formula describing the relative number of each constituting chemical element of the species or molecular entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasProvenance -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasProvenance">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#StandardEnthalpyOfFormation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#Reference"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#EnthalpyOfFormationCalculationEBR"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Weblink"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasReferenceSpecies -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasReferenceSpecies">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#EnthalpyOfFormationCalculationEBR"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#ReferenceSpecies"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines which reference species are used in estimating the value of a property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasReferenceTemperature -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasReferenceTemperature">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#StandardEnthalpyOfFormation"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Temperature"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines the temperature at which the value of a property is estimated.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasSpecies -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasSpecies">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#ReferenceSpecies"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Internationalised Resource Identifier (IRI) of a species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasStandardEnthalpyOfFormation -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasStandardEnthalpyOfFormation">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#StandardEnthalpyOfFormation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines the standard enthalpy of formation of a given species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasWeblink -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasWeblink">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Weblink"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation that defines the weblink of an entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/identifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/elements/1.1/identifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The label or string that identifies an entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.org/dc/elements/1.1</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasNumberOfElement -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl#hasNumberOfElement">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A number that indicates the amount of an element in a species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontokin/OntoKin.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#SMILES -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#SMILES">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Simplified Molecular Input Line Entry System. A form of line notation for codifying the structure of a chemical species using short ASCII strings.  In ChEBI, http://purl.obolibrary.org/obo/chebi/smiles is defined as an annotation property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#casRegistryID -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#casRegistryID">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A numerical identifier designed to uniquely identify all chemical substances. Also in PeriodicTable, http://www.daml.org/2003/01/periodictable/PeriodicTable#casRegistryID is defined as a data type property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#dateOfAccess -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#dateOfAccess">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Weblink"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The date when a web page was accessed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasAtomicBond -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasAtomicBond">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The data property that represents the bonds between all atoms in a species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasGeometry -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#hasGeometry">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The data property that represents the geometry of species.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#inChI -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#inChI">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An identifier designed to codify chemical species, molecules or substances. In ChEBI, http://purl.obolibrary.org/obo/chebi/inchi is defined as an annotation property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#inChIKey -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#inChIKey">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A fixed-length representation of InChI. In ChEBI, http://purl.obolibrary.org/obo/chebi/inchikey is defined as an annotation property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#isReliable -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#isReliable">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#ReferenceSpecies"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The data property that indicates if a species in a given reference set is reliable.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#mass -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#mass">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The combined masses of all atoms of a molecule. In ChEBI, http://purl.obolibrary.org/obo/chebi/mass is defined as an annotation property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#units -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#units">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#StandardEnthalpyOfFormation"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Temperature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The physical units of a quantity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#value -->

    <owl:DatatypeProperty rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#value">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#StandardEnthalpyOfFormation"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Temperature"/>
                    <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Weblink"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The value of a property. Also in OntoCAPE, http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#value is designed as a data type property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel">
        <rdfs:domain rdf:resource="http://www.theworldavatar.com/ontology/ontospecies/OntoSpecies.owl#Species"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An alternative name of a chemical species, molecule or substance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    



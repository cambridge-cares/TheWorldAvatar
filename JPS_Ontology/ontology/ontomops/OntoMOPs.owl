    <owl:Class rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/Unit -->


    <owl:Class rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/Unit"/>
    


    <!-- https://www.theworldavatar.com/kg/ontomops/AssemblyModel -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontomops/AssemblyModel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontomops/hasGenericBuildingUnit"/>
                <owl:allValuesFrom rdf:resource="https://www.theworldavatar.com/kg/ontomops/GenericBuildingUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontomops/hasGenericBuildingUnitNumber"/>
                <owl:allValuesFrom rdf:resource="https://www.theworldavatar.com/kg/ontomops/GenericBuildingUnitNumber"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontomops/hasPolyhedralShape"/>
                <owl:allValuesFrom rdf:resource="https://www.theworldavatar.com/kg/ontomops/PolyhedralShape"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An assembly model represent a spatial constract (i.e. skeleton) of discrete and well-defined interacting generic building units. Many chemical structures may share same assembly model. </rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontomops/</rdfs:isDefinedBy>
    </owl:Class>
    



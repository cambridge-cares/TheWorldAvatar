    <!-- https://www.theworldavatar.com/kg/ontobim/BooleanClippingResult -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/BooleanClippingResult">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasFirstOperand"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasSecondOperand"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a solid that is created as a result of the Boolean difference between a solid (restricted to swept area solid) and a half space solid. The solid is the regularization of the set of all points which are in the first operand/geometry, but not in the second operand. More than one difference operation can be applied to achieve this solid. Its IFC equivalent is IfcBooleanClippingResult.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Boolean Clipping Result</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/CartesianPoint -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/CartesianPoint">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A point defined by its coordinates in a two or three dimensional rectangular Cartesian coordinate system. Its IFC equivalent is IfcCartesianPoint.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cartesian Point</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/CartesianTransformationOperator -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/CartesianTransformationOperator">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasLocalOrigin"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/CartesianPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class defines a geometric transformation in two- or three-dimensional space composed of translation, rotation, mirroring and uniform scaling. Its closest equivalent is IfcCartesianTransformationOperator2D or IfcCartesianTransformationOperator3D.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cartesian Transformation Operator</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/ClosedShell -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/ClosedShell">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a shell of dimensionality 2, that typically serves as a bound for a region. It is usually represented by a collection of faces. Its IFC equivalent is IfcClosedShell.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Closed Shell</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/CompoundPlaneAngle -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/CompoundPlaneAngle">
        <rdfs:subClassOf rdf:resource="https://www.ontology-of-units-of-measure.org/resource/om-2/Angle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A compound measure of the plane angle in degrees, minutes, seconds, and optionally micro-seconds of arc, as defined in IfcCompoundPlaneAngleMeasure. This quantity class is intended to work only for IFC models.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Plane Angle</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/DirectionVector -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/DirectionVector">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a general direction vector in two or three dimensional space based on direction ratios. Its IFC equivalent is IfcDirection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Direction Vector</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/ExtrudedAreaSolid -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/ExtrudedAreaSolid">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/SolidModel"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defined a solid by sweeping a bounded planar surface in a given direction. If the planar area has inner boundaries, i.e. holes defined, then those holes shall be swept into holes of the solid. Its IFC equivalent is IfcExtrudedAreaSolid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extruded Area Solid</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/Face -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/Face">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/TopologicalRepresentation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a topological entity of dimensionality 2, that is represented by its bounding loops or face bounds. Its IFC equivalent is IfcFace.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Face</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/FaceOuterBound -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/FaceOuterBound">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/TopologicalRepresentation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a loop that is the outer boundary of a face. Only one should exist for any given face. Its IFC equivalent is IfcFaceOuterBound.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Face Outer Bound</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/FacetedBrep -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/FacetedBrep">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/SolidModel"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a simple form of boundary representation model in which all faces are planar and all edges are straight lines. Its IFC equivalent is IfcFacetedBrep and this class has to follow the topological constraints of IfcManifoldSolidBrep.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Faceted Brep</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/Facility -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/Facility">
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An independent unit of the built environment, intended to serve at least one function or user activity. A facility is a part of the physical world or a virtual world with a 3D spatial extent that can serve one and more storeys. A facility also can comprise of one or several rooms with different purposes. For example, a multi-storey department store is a facility, which comprise of smaller shopping departments or rooms.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Facility</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A representation of the element that describes the shape of each element, with additional meaning of having geometric position, orientation, or both.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometric Representation </rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationContext -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationContext">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasTrueNorth"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasWorldCoordinateSystem"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/LocalPlacement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a distinct coordinate space, that is spatially unrelated to other coordinate spaces, as the overall representation context in which geometric representation items are geometrically founded. Its IFC equivalent is IfcGeometricRepresentationContext.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometric Representation Context</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationSubContext -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationSubContext">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasParentContext"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationContext"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Define semantically distinguished representation types for different information content, dependent on the representation view and the target scale. This enables the control of the level of detail. As a subcontext, it inherits several attributes from the parent context.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometric Representation SubContext</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/GeometricVoid -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricVoid">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A class that modifies (usually subtracts from) the shape and appearance of the host elements, such as walls, floors, ceilings, and roofs. It is equivalent to the IfcFeatureElementSubtraction or IfcOpeningElement class.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geometric Void</rdfs:label>
    </owl:Class>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/HalfSpaceSolid -->


    <owl:Class rdf:about="https://www.theworldavatar.com/kg/ontobim/HalfSpaceSolid">
        <rdfs:subClassOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A solid that is defined by the half space, ie the regular subset of the domain which lies on one side of an unbounded surface. The surface side in the half space is determined by the surface normal and agreement flag. A valid half space solid must have the domain divided exactly into two subsets by the surface. Its IFC equivalent is IfcHalfSpaceSolid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Half Space Solid</rdfs:label>

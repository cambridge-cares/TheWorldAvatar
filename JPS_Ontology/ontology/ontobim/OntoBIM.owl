        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has z coordinates</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasZDimensionExtent -->



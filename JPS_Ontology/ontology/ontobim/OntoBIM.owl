<?xml version="1.0"?>
<rdf:RDF xmlns="https://www.theworldavatar.com/kg/ontobim/"
     xml:base="https://www.theworldavatar.com/kg/ontobim/"
     xmlns:ontobim="https://www.theworldavatar.com/kg/ontobim/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://www.theworldavatar.com/kg/ontobim/">
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">12 October 2023</dc:date>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Building Information Modelling ontology (OntoBIM) is an ontology developed to represent the detailed information and spatial relationships of buildings and their components (walls, doors, roofs, furniture, equipment) that is available in the Industry Foundation Classes (IFC) data schema. Information about their individual characteristics such as material, readings, and dimensions is combined from IFC or other data sources.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.2</owl:versionInfo>
        <gitCommitHash rdf:datatype="http://www.w3.org/2001/XMLSchema#string">c0599beca8df55873a1ab061dee64e52c510c6a0</gitCommitHash>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/bot#containsZone -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#containsZone"/>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasAxisDirection -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasAxisDirection">
        <rdfs:subPropertyOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasDirection"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/LocalPlacement"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that determines the local placement &amp;apos;s exact direction to the local Z axis.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has axis direction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasBaseSurface -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasBaseSurface">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/HalfSpaceSolid"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/SurfacePlane"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the surface defining side for a half space solid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has base surface</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasBooleanOperator -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasBooleanOperator">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/BooleanClippingResult"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/IfcBooleanOperator"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the operator type for boolean operations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has boolean operator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasBounds -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasBounds">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/Face"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/FaceOuterBound"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the boundaries of a face.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has bounds</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasCompoundPlaneAngle -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasCompoundPlaneAngle">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/IfcSiteRepresentation"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/CompoundPlaneAngle"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links the Site to its world longitude or latitude. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has compound plane angle</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasConnectedFaces -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasConnectedFaces">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/ClosedShell"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/Face"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the closed shell, comprising of a set of faces arcwise connected along common edges or vertices.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has connected faces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasContext -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasContext">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/IfcProjectRepresentation"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentationContext"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that references the project to their geometric representation context.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has context</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasDerivedAxis -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasDerivedAxis">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/CartesianTransformationOperator"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived axis direction, to get a list of mutually orthogonal, normalized vectors defining the transformation matrix in IFC. See IfcCartesianTransformationOperator3D and IfcCartesianTransformationOperator for more information.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has derived axis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasDerivedXAxis -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasDerivedXAxis">
        <rdfs:subPropertyOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasDerivedAxis"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived x-axis direction.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has derived x-axis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasDerivedYAxis -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasDerivedYAxis">
        <rdfs:subPropertyOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasDerivedAxis"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a relationship from the operator to the derived y-axis direction.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has derived y-axis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasDirection -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasDirection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links any entity to a direction vector. This relationship is not intended to be stated explicitly, but inferred from its sub-properties. For application, only the sub-properties are used.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has direction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasExteriorBoundary -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasExteriorBoundary">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/FacetedBrep"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/ClosedShell"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship defining the exterior boundary of the solid through a shell. The shell normal shall point away from the interior of the solid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has exterior boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasExtrusionDirection -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasExtrusionDirection">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/ExtrudedAreaSolid"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the direction of the extrusion of the surface.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has extrusion direction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasExtrusionProfile -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasExtrusionProfile">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/ExtrudedAreaSolid"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/RectangleProfileDefinition"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship defining the surface that is to be extruded, ie profile definition.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has extrusion profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasExtrusionStartPosition -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasExtrusionStartPosition">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/ExtrudedAreaSolid"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/LocalPlacement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the geometric start position of the extruded area solid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has extrusion start position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasFaceBoundary -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasFaceBoundary">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/FaceOuterBound"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/PolyLoop"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the polyloop acting as the boundary for the face.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has face boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasFacility -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/Facility"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relation to facility contained within a building.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasFirstOperand -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasFirstOperand">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/BooleanClippingResult"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/GeometricRepresentation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the first operand/geometry of the boolean clipping operation. This is usually a swept area solid or another boolean clipping result.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has first operand</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasGeometricRepresentation -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasGeometricRepresentation">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricVoid"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcModelRepresentation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/ModelRepresentation3D"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between an element or void within the space or building and their geometric representation defined according to IFC schema.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has geometric representation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasIfcRepresentation -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasIfcRepresentation">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/bot#Building"/>
                    <rdf:Description rdf:about="https://w3id.org/bot#Element"/>
                    <rdf:Description rdf:about="https://w3id.org/bot#Site"/>
                    <rdf:Description rdf:about="https://w3id.org/bot#Storey"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/Room"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcAbstractRepresentation"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcModelRepresentation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a zone or element within a building to their representation in the IFC schema.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has IFC representation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasLocalOrigin -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasLocalOrigin">
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/CartesianTransformationOperator"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/CartesianPoint"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the required translation origin, specified as a cartesian point. The actual translation transform the geometric origin to the local origin.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has local origin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasLocalPosition -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasLocalPosition">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/GeometricVoid"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcAbstractRepresentation"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcModelRepresentation"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/PolygonalBoundedHalfSpace"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/RectangleProfileDefinition"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/SurfacePlane"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/LocalPlacement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the abstract boundaries of a spatial zone or a component within the space or building and their placement position within the model. Note that this does not define their concrete geometric representation positions, which is only available in the ModelRepresentation3D class.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has local position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/hasNextVertex -->


    <owl:ObjectProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/hasNextVertex">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/LineVertex"/>
        <rdfs:range rdf:resource="https://www.theworldavatar.com/kg/ontobim/LineVertex"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that linked the current vertex to the next vertex in a line.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>

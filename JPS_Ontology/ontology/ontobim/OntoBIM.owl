        <rdfs:subPropertyOf rdf:resource="https://www.theworldavatar.com/kg/ontobim/hasDirectionRatio"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/DirectionVector"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship that defines the Direction Vector&amp;apos;s Z-direction ratio as a double value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has z direction ratio</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.theworldavatar.com/kg/ontobim/isLoopNonInversedOrientation -->


    <owl:DatatypeProperty rdf:about="https://www.theworldavatar.com/kg/ontobim/isLoopNonInversedOrientation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.theworldavatar.com/kg/ontobim/FaceOuterBound"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A boolean property that indicates whether the loop has the same sense when used to bound the face as when first defined. When false, the senses of all its component oriented edges are implicitly reversed when used in the face.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.theworldavatar.com/kg/ontobim/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is loop non inversed orientation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://www.w3.org/2000/01/rdf-schema#label -->


    <owl:DatatypeProperty rdf:about="https://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcAbstractRepresentation"/>
                    <rdf:Description rdf:about="https://www.theworldavatar.com/kg/ontobim/IfcModelRepresentation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between the Zone or Element of a building and its reference name that is stored as a string literal such as Basement 1, 1st Floor or Wooden Table.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.w3.org/2000/01/rdf-schema</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">label</rdfs:label>
    </owl:DatatypeProperty>
    



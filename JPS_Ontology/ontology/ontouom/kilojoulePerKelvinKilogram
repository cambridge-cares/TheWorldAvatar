  <!-- Kilojoule Per Kelvin Kilogram Ontology -->

  <om:UnitDivision rdf:about="https://www.theworldavatar.com/kg/ontouom/kilojoulePerKelvinKilogram">
    <rdfs:label xml:lang="en">kilojoule per kelvin kilogram</rdfs:label>
    <om:symbol>kJ/(K kg)</om:symbol>
    <om:alternativeSymbol>kJ/(K·kg)</om:alternativeSymbol>
    <om:alternativeSymbol>kJ K-1 kg-1</om:alternativeSymbol>
    <om:alternativeSymbol>kJ·K-1·kg-1</om:alternativeSymbol>
    <om:hasNumerator rdf:resource="&om;kilojoule"/>
    <om:hasDenominator rdf:resource="&om;kelvinKilogram"/>
    <om:hasDimension rdf:resource="&om;specificEntropyOrSpecificHeatCapacity-Dimension"/>
  </om:UnitDivision>


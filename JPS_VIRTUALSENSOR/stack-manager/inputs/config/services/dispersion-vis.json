{
    "ServiceSpec": {
        "Name": "DispersionVis",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/dtvf-base-image:latest",
                "Mounts": [
                    {
                        "Type": "volume",
                        "Source": "vis_data",
                        "Target": "/var/www/html"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "ui": {
            "url": "http://localhost:80/",
            "externalPath": "/dispersion-vis/"
        }
    }
}
{
    "database": "postgres",
	"skip": false,
    "workspace": "dispersion",
    "datasetDirectory": "churchill_gml",
    "dataSubsets": [
        {
            "type": "CityDB",
            "subdirectory": "subdir",
            "skip": false,
            "importOptions": {
                "sridIn": "27700"
            },
            "name": "building-pirmasens_gsl",
            "geoServerSettings": {
                "virtualTable": {
                    "name": "mapbox_layer",
                    "sql": "@/inputs/config/mapbox-footprint.sql",
                    "escapeSql": false,
                    "geometry": {
                        "name": "geometry",
                        "type": "Polygon",
                        "srid": 27700
                    }
                },
                "defaultStyle": "polygon"
            }
        }
    ],
    "mappings": [
        "citydb.obda"
    ]
}
        {
            "name": "Power stations",
            "stack": "<PUBLIC_URL>",
            "sources": [
                {
                    "id": "tspNodes",
                    "type": "geojson",
                    "cluster": false,
                    "clusterMaxZoom": 14,
                    "clusterRadius": 100,
                    "data": "<PUBLIC_URL>/geoserver/twa/ows?service=WFS&version=1.0.0&request=GetFeature&typeName=twa%3Apoi_tsp_nearest_node&outputFormat=application%2Fjson"
                }
            ],
            "layers": [
                {
                    "id": "tspNodeNormalLayer",
                    "name": "Not flooded",
                    "source": "tspNodes",
                    "type": "symbol",
                    "filter": ["==", ["get", "is_flooded"], false],
                    "order": 50,
                    "layout": {
                        "icon-image": "power-primary",
                        "icon-size": [
                            "interpolate",
                            [
                                "linear"
                            ],
                            [
                                "zoom"
                            ],
                            12,
                            0.5
                        ],
                        "icon-anchor": "bottom"
                    }
                },
                {
                    "id": "tspNodeFloodedLayer",
                    "name": "Flooded",
                    "source": "tspNodes",
                    "type": "symbol",
                    "filter": ["==", ["get", "is_flooded"], true],
                    "order": 51,
                    "layout": {
                        "icon-image": "power-flooded",
                        "icon-size": [
                            "interpolate",
                            [
                                "linear"
                            ],
                            [
                                "zoom"
                            ],
                            12,
                            0.5
                        ],
                        "icon-anchor": "bottom"
                    }
                }
            ]
        },

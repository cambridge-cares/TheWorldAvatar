            ],
            "layers": [
                {
                    "id": "flood-10",
                    "name": "10cm depth",
                    "source": "flood_active_10cm",
                    "source-layer": "flood_polygon_single_10cm",
                    "type": "fill",
                    "order": 1,
                    "layout": {
                        "visibility": "visible"
                    },
                    "paint": {
                        "fill-color": "#0000FF",
                        "fill-opacity": 0.4
                    }
                },
                {
                    "id": "flood-30",

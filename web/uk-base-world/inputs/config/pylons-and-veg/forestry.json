{
    "database": "postgres",
    "workspace": "twa",
    "datasetDirectory": "forestry",
    "skip": false,
    "namespace": {
        "properties": {
            "com.bigdata.rdf.store.AbstractTripleStore.triplesMode": true
        }
    },
    "dataSubsets": [
        {
            "type": "tabular",
            "skip": false,
            "schema": "public",
            "subdirectory": "tabular"
        },
        {
            "type": "vector",
            "skip": false,
            "schema": "public",
            "table": "forestry",
            "subdirectory": "vector",
            "ogr2ogrOptions": {
                "sridOut": "EPSG:27700"
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "forestry",
                    "sql": "@/inputs/config/sql/forestry.sql",
                    "escapeSql": true,
                    "keyColumn": "ogc_fid",
                    "geometry": {
                        "name": "wkb_geometry",
                        "type": "Polygon",
                        "srid": 27700
                    }
                },
                "defaultStyle": "twa:forestry"
            }
        },
        {
            "type": "tboxcsv",
            "subdirectory": "ontology",
            "skip": false
        }
    ],
    "styles": [
        {
            "name": "forestry",
            "file": "forestry.sld"
        }
    ],
    "mappings": [
        "forestry_ontop.obda"
    ]
}
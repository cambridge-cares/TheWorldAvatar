    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#isSubsystemOf -->

    <owl:ObjectProperty rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#isSubsystemOf">

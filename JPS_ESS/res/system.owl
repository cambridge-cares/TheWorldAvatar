        <owl:disjointWith rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#FixedValueSet"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ExtensibleValueSet is a ValueEnumeration which, unlike a FixedValueSet, is not defined by an (exhaustive) enumeration of its instances. Thus, the number of possible values may change at run time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#FirstLevelSubsystem -->

    <owl:Class rdf:about="http://www.theworldavatar.com/ontology/ontocape/upper_level/system.owl#FirstLevelSubsystem">

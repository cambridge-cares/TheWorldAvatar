<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" metadata-complete="false" version="3.1">
    <display-name>java_agent_eg</display-name>
    <description>A simple java agent servlet based on the JPSAgent class that returns the sum of two real numbers.</description>

    <servlet>
        <servlet-name>AdditionAgentServlet</servlet-name>
        <servlet-class>uk.ac.cam.cares.jps.agent.agent_eg.AdditionAgent</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdditionAgentServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
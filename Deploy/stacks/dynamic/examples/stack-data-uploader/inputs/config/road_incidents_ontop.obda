[PrefixDeclaration]
obda:		https://w3id.org/obda/vocabulary#
owl:		http://www.w3.org/2002/07/owl#
ex:			http://example.org/
xsd:		http://www.w3.org/2001/XMLSchema#
ra:			http://www.w3.org/2012/7/ra3.owl#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
geo:		http://www.opengis.net/ont/geosparql#
dc:			http://purl.org/dc/elements/1.1/
xml:		http://www.w3.org/XML/1998/namespace
crops:		http://example.org/crops/
foaf:		http://xmlns.com/foaf/0.1/
twa:		http://theworldavatar.io/
trans:		http://ontology.eil.utoronto.ca/icity/TransportationSystem/

[MappingDeclaration] @collection [[
mappingId	accident-mapping
target		ra:RoadAccident/{ogc_fid} a ra:RoadAccident ;
				ra:location ra:RoadAccident/location/{ogc_fid} .
			ra:RoadAccident/location/{ogc_fid} a geo:Feature ;
				a ra:Location ;
				geo:hasDefaultGeometry ra:RoadAccident/geom/0/{ogc_fid} .
			ra:RoadAccident/geom/0/{ogc_fid} geo:hasWKT "<http://www.opengis.net/def/crs/EPSG/0/4326> {wkt}"^^geo:wktLiteral .
source		SELECT ogc_fid, ST_ASTEXT(geom) as wkt
			FROM "road_incidents"

mappingId   pubs-mapping
target      twa:PublicHouse/{ogc_fid} a a geo:Feature/;
			geo:hasDefaultGeometry geo:hasWKT "<http://www.opengis.net/def/crs/EPSG/0/4326> {wkt}"^^geo:wktLiteral .
source		select ogc_fid, ST_ASTEXT(geom) as wkt

mappingId	road-mapping
target		trans:Road/{ogc_fid} a trans:Road;
			geo:hasDefaultGeometry geo:hasWKB "<http://www.opengis.net/def/crs/EPSG/0/27700> {geom}"^^geo:wktLiteral
			
source		select ogc_fid, st_asewkt(st_asewkt(wkb_geometry) as geom) from roads
]]
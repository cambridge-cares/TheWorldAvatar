[PrefixDeclaration]
obda:		https://w3id.org/obda/vocabulary#
owl:		http://www.w3.org/2002/07/owl#
ex:			http://example.org/
xsd:		http://www.w3.org/2001/XMLSchema#
ra:			http://www.w3.org/2012/7/ra3.owl#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
geo:		http://www.opengis.net/ont/geosparql#
dc:			http://purl.org/dc/elements/1.1/
xml:		http://www.w3.org/XML/1998/namespace
crops:		http://example.org/crops/
foaf:		http://xmlns.com/foaf/0.1/
twa:		http://theworldavatar.io/
trans:		http://ontology.eil.utoronto.ca/icity/TransportationSystem/
pubs:		http://theworldavatar.com/ontology/pubontology.owl#

[MappingDeclaration] @collection [[

mappingId   pubs-mapping
target      twa:PublicHouse/{ogc_fid} a geo:Feature/;
			geo:hasDefaultGeometry twa:PublicHouse/geom/{ogc_fid} ;
			geo:hasWKT "<http://www.opengis.net/def/crs/EPSG/0/4326> {wkt}"^^geo:wktLiteral ;
			rdfs:label {name} ;
			twa:hasLocalAuthority {local_authority} ;
			pubs:hasDisplayAddress {address} .
source		SELECT name, address, local_authority, ogc_fid, ST_ASTEXT(geom) as wkt
			FROM "open_pubs"

]]
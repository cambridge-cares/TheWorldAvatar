{
    "name": "public_houses",
    "datasetDirectory": "public_houses",
    "database": "postgres",
    "workspace": "the_world_avatar",
    "skip": false,
    "dataSubsets": [
        {
            "type": "vector",
            "skip": false,
            "schema": "public",
            "subdirectory": "public_houses",
            "table": "open_pubs",
            "geoServerSettings": {
                "virtualTable": {
                    "name": "pubs_virtual",
                    "sql": "@/inputs/config/pubs.sql",
                    "escapeSql": false,
                    "keyColumn": "ogc_fid",
                    "geometry": {
                        "name": "geom",
                        "type": "Point",
                        "srid": 27700
                    }
                }
            },
            "ogr2ogrOptions": {
                "layerCreationOptions": {
                    "GEOMETRY_NAME": "geom"
                },
                "sridIn": "EPSG:4326",
                "sridOut": "EPSG:27700",
                "inputDatasetOpenOptions": {
                    "X_POSSIBLE_NAMES": "longitude",
                    "Y_POSSIBLE_NAMES": "latitude"
                }
            }
        }
    ],
    "staticGeoServerData": {
        "iconsDir": "icons"
    },
    "mappings": [
        "pubs_ontop.obda"
    ]
}
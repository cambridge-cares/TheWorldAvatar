{
    "ServiceSpec": {
        "Name": "visualisation",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/dtvf-base-image:latest",
                "Mounts": [
                    {
                        "Type": "volume",
                        "Source": "vis-files",
                        "Target": "/var/www/html"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "ui": {
            "url": "http://localhost/",
            "externalPath": "/visualisation/"
        }
    }
}
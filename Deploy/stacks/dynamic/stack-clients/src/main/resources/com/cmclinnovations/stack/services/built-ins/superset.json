{
    "type": "superset",
    "ServiceSpec": {
        "Name": "superset",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "apache/superset:2.0.1",
                "Env": [
                    "APP_NAME=Dashboard",
                    "PUBLIC_ROLE_LIKE=Gamma"
                ],
                "Secrets": [
                    {
                        "SecretName": "superset_password"
                    },
                    {
                        "SecretName": "superset_secret_key"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "ui": {
            "url": "http://localhost:8088/",
            "externalPath": "/analytics"
        }
    }
}
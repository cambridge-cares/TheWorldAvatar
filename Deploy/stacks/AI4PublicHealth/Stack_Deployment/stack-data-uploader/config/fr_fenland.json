{
    "database": "postgres",
    "workspace": "the_world_avatar",
    "datasetDirectory": "fr_fenland",
    "skip": false,
    "dataSubsets": [
        {
            "type": "vector",
            "skip": false,
            "schema": "public",
            "table": "fr_fenland",
            "subdirectory": "vector",
            "ogr2ogrOptions": {
                "layerCreationOptions": {
                    "GEOMETRY_NAME": "geom",
                    "SEPARATOR": "COMMA"
                },
                "sridIn": "EPSG:4326",
                "sridOut": "EPSG:4326",
                "inputDatasetOpenOptions": {
                    "X_POSSIBLE_NAMES": "Longitude",
                    "Y_POSSIBLE_NAMES": "Latitude"
                }
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": " fr_fenland_extended",
                    "sql": "SELECT geom, \"Name\", \"Address\", \"Postcode\", \"Coding\", \"Council\" FROM  fr_fenland",
                    "escapeSql": false,
                    "geometry": {
                        "name": "geom",
                        "type": "Point",
                        "srid": 4326
                    }
                }
            }
        }
    ]
}
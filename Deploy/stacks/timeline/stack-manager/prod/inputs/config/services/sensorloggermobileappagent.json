{
    "ServiceSpec": {
        "Name": "sensorloggermobileappagent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/cambridge-cares/sensorloggermobileappagent:4.0.0-dev-add-session-id-SNAPSHOT",
                "Mounts": [
                    {
                        "Type": "bind",
                        "Source": "sensor-logger-resources",
                        "Target": "/resources"
                    }
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    },
                    {
                        "ConfigName": "geoserver"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    },
                    {
                        "SecretName": "geoserver_password"
                    }
                ]
            }
        }
    },
    "endpoints": {
        "isochroneagent": {
            "url": "http://localhost:8080/sensorloggermobileappagent/",
            "externalPath": "/sensorloggermobileappagent/"
        }
    }
}
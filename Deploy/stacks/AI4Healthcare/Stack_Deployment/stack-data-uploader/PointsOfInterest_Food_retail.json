{
    "database": "postgres",
    "workspace": "the_world_avatar",
    "datasetDirectory": "PointsOfInterest",
    "skip": false,
    "dataSubsets": [
        {
            "type": "vector",
            "skip": false,
            "schema": "public",
            "table": "points_of_interest_ex_supermarkets",
            "subdirectory": "vector",
            "ogr2ogrOptions": {
                "layerCreationOptions": {
                    "GEOMETRY_NAME": "geom",
                    "SEPARATOR": "COMMA"
                },
                "sridIn": "EPSG:27700",
                "sridOut": "EPSG:4326",
                "inputDatasetOpenOptions": {
                    "X_POSSIBLE_NAMES": "feature_easting",
                    "Y_POSSIBLE_NAMES": "feature_northing"
                }
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "points_of_interest_extended",
                    "sql": "SELECT geom, \"name\", \"pointx_class\", \"feature_easting\", \"feature_northing\", \"shared_word\", \"postcode\" FROM points_of_interest_ex_supermarkets",
                    "escapeSql": false,
                    "geometry": {
                        "name": "geom",
                        "type": "Point",
                        "srid": 4326
                    }
                }
            }
        }
    ],
    "mappings": [
        "PointsOfInterest.obda"
    ]
}
{
    "database": "postgres",
    "name": "building-kaiserslautern",
    "skip": false,
    "workspace": "dispersion-kaiserslautern",
    "datasetDirectory": "buildings",
    "dataSubsets": [
        {
            "type": "CityDB",
            "subdirectory": "buildings",
            "skip": false,
            "importOptions": {
                "sridIn": "25832"
            },
            "name": "building-kaiserslautern_gsl",
            "geoServerSettings": {
                "virtualTable": {
                    "name": "mapbox_layer",
                    "sql": "@/inputs/config/mapbox-footprint.sql",
                    "escapeSql": false,
                    "geometry": {
                        "name": "geometry",
                        "type": "Polygon",
                        "srid": 25832
                    }
                },
                "defaultStyle": "polygon"
            }
        }
    ]
}
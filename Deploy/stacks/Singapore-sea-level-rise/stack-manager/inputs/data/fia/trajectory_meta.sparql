PREFIX ontodevice: <https://www.theworldavatar.com/kg/ontodevice/>
PREFIX ontobuiltenv: <https://www.theworldavatar.com/kg/ontobuiltenv/>

SELECT ?Type (COUNT(?Type) AS ?Count)
WHERE {
  SERVICE [ONTOP] { 
    [FEATURE_IRI_VALUES]
    
    ?building ontobuiltenv:hasPropertyUsage ?usage.
    ?usage a ?Type.
  }
}
GROUP BY ?Type
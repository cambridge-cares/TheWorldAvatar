ps:PirmasensStation_Stickstoffmonoxid rdfs:label "Stickstoffmonoxid"^^xsd:string ;
	om:hasUnit om:microgramPerCubicMetre .

ps:PirmasensStation_Temperatur rdfs:label "Temperatur"^^xsd:string ;
